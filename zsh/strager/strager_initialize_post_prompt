strager_initialize_post_prompt() {
    strager_initialize_completion
    strager_initialize_history
}

strager_initialize_completion() {
    autoload -Uz compinit
    compinit
}

strager_initialize_history() {
    if [[ "${STRAGER_HISTFILE:+set}" = set ]]; then
        fc -p "${STRAGER_HISTFILE}"
        unset STRAGER_HISTFILE
    fi
}
